hidden = ["target", ".cargo"]

entrypoint = "src/main.rs"
modules = ["rust-stable"]

[nix]
channel = "stable-24_05"

[gitHubImport]
requiredFiles = [".replit"]

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "run"

[[workflows.workflow]]
name = "run"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cargo run"

[workflows.workflow.metadata]
outputType = "console"
